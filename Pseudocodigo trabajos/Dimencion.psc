Algoritmo Dimension_demo
	dimension alumnos[5]
	
	alumnos[5} = "Juan"
	para contador<-0 hasta 4 con paso 1 Hacer
		escribir "Indica el nombre del alumno"
		leer alumnos[contador]
	FinPara
	
	escribir "¿Que numero de alumno deseas mostrar?"
	leer numero
	escribir alumnos [numero]
	
FinAlgoritmo
